{
  "name": "firescript",
  "word": "identifier",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_simple_definitions"
      }
    },
    "_simple_definitions": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "const_variable"
        },
        {
          "type": "SYMBOL",
          "name": "let_variable"
        }
      ]
    },
    "const_variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "const"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "let_variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "let"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "literal": {
      "type": "PATTERN",
      "value": "'.*'"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z$][a-zA-Z0-9$_]*"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+(\\.\\d+)?"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "indent"
    },
    {
      "type": "SYMBOL",
      "name": "dedent"
    },
    {
      "type": "SYMBOL",
      "name": "_newline"
    }
  ],
  "inline": [],
  "supertypes": []
}
